
public class Gragh {
	static int maxn=105;
	static int t,n,m;
	static int G[]=new int[maxn];				//存储有向图
	static int topo[]=new int[maxn];			//拓扑序列
	static int vis[]=new int[maxn];	//标记该节点状态
	//v[i]=0,该节点尚未被访问；
	//v[i]=1该节点已经被访问，并且还完成了该节点的dfs(i)
	//v[i]=-1表示该节点正在被访问。  这些标志可以用来判断图中是否有环
	
	static boolean dfs(int u) {
		vis[u]=-1;
		for(int v=1;v<=n;v++) {
			if(G[u][v]==1)
		}
	}
}
