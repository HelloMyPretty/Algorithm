import java.util.Arrays;

public class ´î»ýÄ¾ {
	boolean[] vis = new boolean[10];
	int map[] = new int[10];
	int ans;
	
	void print() {
		System.out.println(Arrays.toString(map));
	}
	void dfs(int n) {
		if(n>=10) {
			print();
			System.out.println();
			return;
		}
		for(int i=0;i<=9;i++) {
			if(vis[i]) continue;
			map[n]=i;
			dfs(n+1);
			vis[i]=false;
		}
	}


	public static void main(String[] args) {

		´î»ýÄ¾ test = new ´î»ýÄ¾();
		Arrays.fill(test.vis, false);
		test.dfs(0);
	}
