
public class FloydWarshell {
	static int maxn;
	int dis[][]=new int[maxn][maxn];		//权值矩阵
	int gra[][]=new int[maxn][maxn];		//邻接矩阵,初始化为INF,令gra[i][i]=0
	int path[][]=new int[maxn][maxn];
	int n;									//n个结点
	void floyd() {
		for(int i=0;i<n;i++) {
			for(int j=0;j<n;j++) {
				dis[i][j]
			}
		}
	}
	
}
