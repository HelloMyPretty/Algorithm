package µÚ126th;

public class BSolution {
	public boolean isValid(String S) {
		StringBuilder cur=new StringBuilder(S),next;
		String string="abc";
		boolean ok=true;
		while(ok) {
			int idx=cur.indexOf(string);
			if(idx==cur.length())
				ok=false;
			cur=cur.delete(idx, idx+3);
		}
		return ok;
	}
}
