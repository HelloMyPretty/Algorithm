
public class Phi {
	static int euler(int n) {
		int m = (int) Math.sqrt(0.5 + n);
		int ans = n;
		for (int i = 2; i <= m; i++) {
			if (n % i == 0) {
				ans = ans / i * (i - 1);
				while (n % i == 0)
					n /= i;
			}
		}

		if (n > 1)
			ans = ans / n * (n - 1);
		return ans;
	}
	public static void main(String[] args) {
		System.out.println(euler(10));
	}
}
