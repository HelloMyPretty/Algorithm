package µÚÆß½ì;

import java.util.Arrays;

public class ´î»ýÄ¾ {
	//boolean[] vis = new boolean[10];
	int map[][] = new int[5][5];
	int ans;
	
	void print() {
		for(int i=1;i<=4;i++)
			for(int j=1;j<=i;j++) {
				System.out.print(map[i][j]+" ");
			}
		System.out.println();
	}
	void dfs(int floor,boolean[] vis) {
		if(floor==5) {
			ans++;
			print();
			return;
		}
		boolean ok=true;
		label:
		for(int i=1;i<=floor;i++) {
			if(!ok) return;
			for(int j=0;j<10;j++) {
				if(!vis[j]&&map[floor-1][i-1]<j&&map[floor-1][i]<j)
				{
					map[floor][i]=j;
					vis[j]=true;
					continue label;
				}
			}
			ok=false;
		}
		dfs(floor+1, vis);	
	}


	public static void main(String[] args) {

		´î»ýÄ¾ test = new ´î»ýÄ¾();

		for(int i=0;i<10;i++) {
			boolean[] vis=new boolean[10];
			vis[i]=true;
			test.map[1][1]=i;
			test.dfs(2,vis);
		}
	}
}
